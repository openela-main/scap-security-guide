diff -ruN orig/shared/checks/oval/installed_OS_is_rl9.xml scap-security-guide-0.1.60/shared/checks/oval/installed_OS_is_rl9.xml
--- orig/shared/checks/oval/installed_OS_is_rl9.xml	1969-12-31 19:00:00.000000000 -0500
+++ scap-security-guide-0.1.60/shared/checks/oval/installed_OS_is_rl9.xml	2022-05-28 12:46:38.210608347 -0400
@@ -0,0 +1,48 @@
+<def-group>
+  <definition class="inventory"
+  id="installed_OS_is_rl9" version="1">
+    <metadata>
+      <title>OpenELA 9</title>
+      <affected family="unix">
+        <platform>multi_platform_all</platform>
+      </affected>
+      <reference ref_id="cpe:/o:openela:openela_linux:9"
+      source="CPE" />
+      <description>The operating system installed on the system is
+      OpenELA 9</description>
+    </metadata>
+    <criteria operator="AND">
+      <extend_definition comment="Installed OS is part of the Unix family"
+      definition_ref="installed_OS_is_part_of_Unix_family" />
+      <criterion comment="OS is OpenELA" test_ref="test_openela9_name" />
+      <criterion comment="OS version is 9" test_ref="test_openela9_version" />
+    </criteria>
+  </definition>
+
+  <ind:textfilecontent54_test check="all" check_existence="at_least_one_exists" comment="Check os-release ID" id="test_openela9_name" version="1">
+    <ind:object object_ref="obj_name_openela9" />
+    <ind:state state_ref="state_name_openela9" />
+  </ind:textfilecontent54_test>
+  <ind:textfilecontent54_object id="obj_name_openela9" version="1" comment="Check os-release ID">
+    <ind:filepath>/etc/os-release</ind:filepath>
